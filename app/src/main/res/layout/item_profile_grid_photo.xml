<?xml version="1.0" encoding="utf-8"?>
<com.star.pibbledev.home.createmedia.mediapicker.ui.component.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/iv_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_margin="0.5dp"
        android:background="@color/line_background_color"
        android:scaleType="centerCrop" />

    <ImageView
        android:id="@+id/img_post_type"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_margin="5dp"
        android:src="@drawable/icon_type_multi_image" />


</com.star.pibbledev.home.createmedia.mediapicker.ui.component.SquareRelativeLayout>